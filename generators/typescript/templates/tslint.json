{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "trailing-comma": false,
    "arrow-parens": false,
    "triple-equals": false,
    "no-default-export": true,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "max-line-length": [ true, { "limit": 140, "ignore-pattern": "^import |//" } ],
    "no-angle-bracket-type-assertion": false,
    "no-var-requires": false,
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-type",
      "check-preblock"
    ]
  },
  "rulesDirectory": []
}